{
  "name": "webpack-sass",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --inline --env development\"",
    "build": "SET NODE_ENV=development&&webpack --progress --colors --watch",
    "dev": "webpack-dev-server --hot --inline",
    "production": "SET NODE_ENV=production&&webpack -p"
  },
  "devDependencies": {
    "angular": "^1.5.7",
    "angular-route": "^1.6.4",
    "angular-sanitize": "^1.6.4",
    "angular-ui-router": "^0.4.2",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.25.0",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "ng-annotate-loader": "^0.2.0",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.3.3"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "es2015",
      "stage-0"
    ]
  }
}
